{ 
    "manifest_version": 2, 
    "name": "Chrome Url Shortener", 
    "description": "Shorten urls in wichever tab you are in.", 
    "version": "1", 
    "author": "Kyle Pulido, Harriet Akot", 
    "browser_action": {   
                        "default_icon": "chrome/icons/tab-icon.png",   
                        "default_title": "shorten your urls",
                        "default_popup": "chrome/html/popup.html"
                    }, 
    "permissions": ["activeTab"]
}
